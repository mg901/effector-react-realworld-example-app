{
  "homepage": "https://mg901.github.io/effector-react-realworld-example-app",
  "name": "effector-react-realworld-example-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "start": "cross-env webpack-dev-server",
    "build": "npm run clean && NODE_ENV=production webpack",
    "clean": "cross-env rimraf public",
    "commit": "cross-env git-cz",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:all": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env fixme=fixme prettier --write '**/*.{j,t}{s,sx}'",
    "pretty:watch": "cross-env fixme=fixme onchange '**/*.{j,t}{s,sx}' -- prettier --write {{changed}}",
    "ts-check": "cross-env tsc",
    "ts-check:watch": "yarn ts-check -- --watch",
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch",
    "demo": "npx serve -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "public/bundle.*.js",
      "limit": "76kb"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@size-limit/preset-app": "^4.7.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/history": "4.7.7",
    "@types/jest": "^26.0.15",
    "@types/marked": "^1.1.0",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.2.2",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.0",
    "node-static": "^0.7.11",
    "onchange": "^7.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "size-limit": "^4.7.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.0.5",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "effector": "^21.6.2",
    "effector-forms": "^0.0.18",
    "effector-react": "^21.0.8",
    "effector-root": "^1.1.0",
    "effector-storage": "^3.0.0",
    "features": "^0.1.0",
    "history": "4.10.1",
    "markdown-to-jsx": "^7.0.1",
    "patronum": "^0.111.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0"
  }
}
