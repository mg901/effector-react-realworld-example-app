{
  "homepage": "https://mg901.github.io/effector-react-realworld-example-app",
  "name": "effector-react-realworld-example-app",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "predeploy": "npm run build",
    "preview": "npx serve -s build",
    "prepare": "husky install",
    "commit": "cross-env git-cz",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "npm run clean && NODE_ENV=production webpack",
    "clean": "cross-env rimraf build",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env prettier --write '**/*.{j,t}{s,sx}'",
    "pretty:watch": "cross-env onchange '**/*.{j,t}{s,sx}' -- prettier --write {{changed}}",
    "typecheck": "cross-env tsc",
    "typecheck:watch": "yarn typecheck -- --watch",
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "build/*.js",
      "limit": "129kb"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@size-limit/preset-app": "^7.0.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.4.0",
    "@types/marked": "^4.0.1",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-effector": "^0.5.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "jest-environment-jsdom": "^27.4.4",
    "lint-staged": "^12.1.3",
    "mini-css-extract-plugin": "^2.4.5",
    "node-static": "^0.7.11",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "query-string": "^7.0.1",
    "react-polymorphic-types": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.5",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "effector": "^22.1.2",
    "effector-react": "^22.0.6",
    "effector-storage": "^4.5.0",
    "history": "4.10.1",
    "markdown-to-jsx": "^7.1.5",
    "patronum": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.3",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "use-query-params": "^1.2.3"
  }
}
